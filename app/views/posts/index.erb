<h1>POSTS INDEX PAGE</h1>


<% @posts.each do |post| %>
    <p>
        <%=@posts.index(post)+1%>. <a href="/posts/<%=post.id%>"> <%=owner_name(post)%> <%=car_type(post)%> to go from <%=post.origin%> to <%=post.destination%></a> 
        <br/>
        <sub>posted <%=time_since_post(post.created_at)%></sub>
    </p>
<% end %>

<p><a href="/posts/new">Create Post</a></p> 